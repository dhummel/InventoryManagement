<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New ProductCatalog</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @productcatalog.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Name: </td>
                <td class="itemValue"><input type="text" name="productcatalog[Name]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Sku: </td>
                <td class="itemValue"><input type="text" name="productcatalog[SKU]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Image: </td>
                <td class="itemValue"><input type="text" name="productcatalog[Image]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
